@model FrontendCNE.Models.DepartamentoViewModel

<h4>DepartamentoViewModel</h4>
<hr />
<div class="row">
    <form asp-action="Edit">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="row">
            <div class="col-md-6">
                <label asp-for="Dep_Id" class="control-label"></label>
                <input asp-for="Dep_Id" class="form-control" id="id"  name="id"/>
                <span asp-validation-for="Dep_Id" class="text-danger"></span>
            </div>

            <div class="col-md-6">
                <label asp-for="Dep_Descripcion" class="control-label"></label>
                <input asp-for="Dep_Descripcion"  name="Descripcion" class="form-control" />
                <span asp-validation-for="Dep_Descripcion" class="text-danger"></span>
            </div>
        </div>

        <div class="form-group">
            <input type="submit" value="Guardar" class="btn btn-primary" />
            <a asp-action="Index" class="btn btn-secondary">Cancelar</a>


        </div>
    </form>

</div>

